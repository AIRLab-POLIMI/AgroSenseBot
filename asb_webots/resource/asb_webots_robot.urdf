<?xml version="1.0" ?>
<robot name="asb_webots_robot">
    <webots>
        <device reference="base_scan_front" type="Lidar">
            <ros>
                <enabled>true</enabled>
                <updateRate>10</updateRate>
                <topicName>/scan_front</topicName>
                <alwaysOn>false</alwaysOn>
                <frameName>base_scan_front</frameName>
            </ros>
        </device>

        <device reference="gnss_link" type="GPS">
            <ros>
                <topicName>/gps/fix</topicName>
                <frameName>gnss_link</frameName>
            </ros>
        </device>

        <plugin type="asb_webots_driver::ASBWebotsDriver" />
    </webots>

</robot>